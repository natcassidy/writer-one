{
  "functions": [
    {
      "source": "functions",
      "codebase": "default",
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log"
      ]
    }
  ],
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/privacy-policy",
        "destination": "/privacy-policy.html"
      },
      {
        "source": "/addToDoc/**",
        "function": "plugin"
      },
      {
        "source": "/getCompletedDoc/**",
        "function": "plugin"
      },
      {
        "source": "/documents/**",
        "function": "plugin"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}